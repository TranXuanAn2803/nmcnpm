<div class="container">
  <div style="display:flex; justify-content:flex-end">
    <a href="/DaiLy/add" style="color:white;"><button
        type="button"
        class="btn btn-primary waves-effect waves-light"
        style="margin-bottom: 10px;"
      >Thêm Đại Lý</button></a>
  </div>

  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-body">

          <table
            id="datatable-fixed-header"
            class="table table-striped table-bordered"
          >
            <thead>
              <tr>
                <th class="text-center">Tên</th>
                <th class="text-center" style="width:10%">Loại Đại Lý</th>
                <th class="text-center" style="width:10%">Quận</th>

                <th class="text-center" style="width:20%">Tiền Nợ</th>

              </tr>
            </thead>

            <tbody>
              {{#each daiLy}}
                <tr>
                  <td> <a href="/DaiLy/{{ID}}">{{Name}}</a></td>
                  <td> <a href="/DaiLy/{{ID}}">{{Type}}</a></td>
                  <td> <a href="/DaiLy/{{ID}}">{{District}}</a></td>
                  <td> <a href="/DaiLy/{{ID}}">{{Debts}}</a></td>

                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>